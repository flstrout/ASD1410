var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#000"
});

var graphic1 = Ti.UI.createView({
	backgroundColor: "#f40",
	top: 25,
	right: 10,
	width: 265,
	height: 25,
	borderRadius: 11
});

var graphic2 = Ti.UI.createView({
	backgroundColor: "#f40",
	top: 25,
	left: 10,
	width: 80,
	height: 430,
	borderRadius: 37
});

mainWindow.add(graphic1, graphic2);
mainWindow.open();
