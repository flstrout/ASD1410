var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#000"
});

var graphic1 = Ti.UI.createView({
	backgroundColor: "#f40",
	top: 25,
	right: 10,
	width: 265,
	height: 25,
	borderRadius: 12
});

var graphic2 = Ti.UI.createView({
	backgroundColor: "#f40",
	top: 25,
	left: 10,
	width: 100,
	height: 400,
	borderRadius: 50
});

mainWindow.add(graphic1, graphic2);
mainWindow.open();
