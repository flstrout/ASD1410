var objectLoop = function(){
	for (i=1; j=10; i>j, i++){
		var button1 = Ti.UI.createLabel({
			text: customer[i].name,
			pass: customer[i],
			font: {fontSize: sizeFont},
			top: 10,
			height: 40,
			width: buttonWidth,
			backgroundColor: "#fff",
			borderColor: "#222",
			borderRadius: 3,
			textAlign: "center"
		});
		if (i <6){
			button1.left = 15;
		} else {
			button1.right = 15;
		};
		button1.addEventListener("click", openAlert);
		mainWindow.add(button1);
	};
};
